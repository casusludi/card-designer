{
    "include":["src/**/*"],
    "compilerOptions": {
        "target": "es2020",
        "module": "esnext",
        "moduleResolution": "node",
    
        "strict": true,
        "noUnusedLocals": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
    
        "inlineSources": true,
        "sourceMap": true,
    
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
    
        "newLine": "lf",
    
        "noEmitOnError": true,
        "jsx": "react",
        "jsxFactory": "React.createElement",
        "resolveJsonModule": true,
        
    }
}